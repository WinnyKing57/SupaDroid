<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <!-- TODO: Remove this file if you don't need to customize data extraction rules.
         For more information about this file, see
         https://developer.android.com/guide/topics/data/data-extraction-rules
    -->
    <cloud-backup>
        <include
            domain="database"
            path="." />
        <include
            domain="sharedpref"
            path="." />
        <exclude
            domain="sharedpref"
            path="device.xml" />
    </cloud-backup>
    <device-transfer>
        <include
            domain="database"
            path="." />
        <include
            domain="sharedpref"
            path="." />
        <exclude
            domain="sharedpref"
            path="device.xml" />
    </device-transfer>
</data-extraction-rules>
